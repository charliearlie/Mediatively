{
  "name": "show-tracker-react",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "client": "npm start --prefix client",
    "buildclient": "npm build --prefix client",
    "server": "node ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "build": "concurrently \"npm run server\" \"npm run buildclient\"",
    "test": "npm run server",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "debug": "~2.6.9",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "express": "~4.15.5",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "request": "^2.83.0"
  }
}
